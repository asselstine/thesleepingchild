						}
					}

					updatePrice();
				});

				$("#units").bind("change", function() {

					if (this.selectedIndex+1 > 1) {
						$("#duration option[value='1']").remove();
					}
					else if ($("#duration option[value='1']").length === 0) {
						$("#duration").append("<option value='1'>1 hour</option>");
					}

					updatePrice();
				});

				// We need an array of localised numbers for the slots drop down.
				var localisedUnits = [

					"1",

					];

				remail = /^[\s,;]*[-a-z0-9~!$%^&*_=+}{\'?]+(\.[-a-z0-9~!$%^&*_=+}{\'?]+)*@([a-z0-9][-a-z0-9]*(\.[-a-z0-9]+)*\.([a-z]{2,})|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(:[0-9]{1,5})?([\s,;]+[-a-z0-9~!$%^&*_=+}{\'?]+(\.[-a-z0-9~!$%^&*_=+}{\'?]+)*@([a-z0-9][-a-z0-9]*(\.[-a-z0-9]+)*\.([a-z]{2,})|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(:[0-9]{1,5})?)*[\s,;]*$/i;
				rphone = /^\+?[0-9\(\)\- ]*[0-9][0-9][0-9\(\)\- ]*(?:\(?(?:e\.?|x\.?|ex\.?|ext\.?|exten\.?|extension) ?[0-9]+\)?)?$/;
				r3 = /^[0-9]{3}$/;
				r4 = /^[0-9]{4}$/;
				rgroupon = /^[0-9A-F]{10}$/;

				$("#theform").submit(function() {
					success = true;



					inp = $("input[name=field1]");

					inp.removeClass("invalid");
