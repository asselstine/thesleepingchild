			(function($){

				$("#duration").bind("change", function() {

					if (this.selectedIndex === 0) {

						$(".updateEnd").html("Wednesday, May 20, 2015 9:00 AM");
						if ($("#units").size() > 1) {
							while ($("#units option").size() > 1) $("#units option:last").remove();
